                                        <xs:documentation>If MemberID provided, then cannot provide other member information.
                                        </xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                            </xs:sequence>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="EligibilityPeriod" type="common:ExpirableType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>If no date provided, the assumes current eligibility.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="RtrvMbrMaterialsRespPayloadType">
        <xs:annotation>
